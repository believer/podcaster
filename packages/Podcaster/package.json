{
  "name": "podcaster",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "bsb -clean-world",
    "start": "cross-env BS_WATCH_CLEAR=true bsb -make-world -w",
    "build": "bsb -clean-world -make-world",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "generate": "npx get-graphql-schema http://localhost:4000/graphql -j > graphql_schema.json",
    "dev": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "svg:cleanup": "yarn trash src/SVGs/components",
    "svg:convert": "react-from-svg src/SVGs src/SVGs/components --with-reason",
    "svg:format": "find src/SVGs/components -name \"*.re\" | grep -v \"node_modules\" | xargs bsrefmt --in-place",
    "svg": "npm run svg:cleanup && npm run svg:convert && npm run svg:format"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@baransu/graphql_ppx_re": "0.7.1",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-community/slider": "2.0.8",
    "@react-navigation/bottom-tabs": "5.0.0-alpha.34",
    "@react-navigation/core": "5.0.0-alpha.32",
    "@react-navigation/native": "5.0.0-alpha.24",
    "@react-navigation/stack": "5.0.0-alpha.58",
    "react": "16.9.0",
    "react-from-svg": "3.1.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.2.0",
    "react-native-svg": "12.0.3",
    "react-native-video": "5.0.2",
    "reason-apollo": "0.18.0",
    "reason-apollo-hooks": "6.0.0",
    "reason-react": "0.7.0",
    "reason-react-native": "0.61.1",
    "reason-react-navigation": "5.0.0-beta.0",
    "track-duration": "0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@react-native-community/eslint-config": "^0.0.7",
    "babel-jest": "^25.1.0",
    "bs-let": "0.1.16",
    "bs-platform": "7.2.1",
    "cross-env": "7.0.2",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.9.0",
    "trash-cli": "3.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
